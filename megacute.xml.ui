<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkWindow" id="window">
    <property name="width-request">360</property>
    <property name="height-request">200</property>
    <property name="default-width">640</property>
    <property name="default-height">480</property>
    <child>
      <object class="GtkPaned">
        <property name="start-child">
          <object class="GtkFrame">
            <child>
              <object class="GtkBox">
                <property name="margin-top">10</property>
                <property name="margin-bottom">10</property>
                <property name="orientation">1</property>
                <property name="spacing">10</property>
                <property name="valign">0</property>
                <property name="vexpand">true</property>
                <child>
                  <object class="GtkEntry">
                    <property name="valign">1</property>
                    <property name="name">searchbox</property>
                  </object>
                </child>
                <child>
                  <object class="GtkScrolledWindow">
                    <property name="vexpand">true</property>
                    <property name="hscrollbar-policy">2</property>
                    <property name="propagate-natural-height">true</property>
                    <property name="has-frame">true</property>
                    <property name="valign">1</property>
                    <child>
                      <object class="GtkGridView" id="icons">
                        <property name="orientation">1</property>
                        <property name="max-columns">2</property>
                        <property name="min-columns">2</property>
                        <property name="hexpand">true</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkEntry">
                    <property name="valign">2</property>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </property>
        <property name="end-child">
          <object class="GtkFrame">
            <child>
              <object class="GtkImage" id="image"></object>
            </child>
          </object>
        </property>
      </object>
    </child>
  </object>
</interface>
